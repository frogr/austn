    "@react-three/drei": "^10.0.4",
    "@react-three/fiber": "^9.1.0",
    "@types/three": "^0.174.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "three": "^0.174.0"
  }
}
